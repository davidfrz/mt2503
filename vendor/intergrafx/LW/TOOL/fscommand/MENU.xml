<ROOT>
	<SYSTEM Width="1024"
			Height="768"
			LastReleaseDate="2011_03_30"
			Ver="1.2.0"
			Company="InterGrafx">
		<SETTING>
			<WP enName="Mimosa"			ONOFF="true"/>
			<WP enName="Girl"			ONOFF="true"/>
			<WP enName="Clock"			ONOFF="true"/>
			<WP enName="Horse"			ONOFF="true"/>
			<WP enName="Daydream"		ONOFF="true"/>
			<WP enName="Lighthouse"		ONOFF="true"/>
			<WP enName="Puppy"			ONOFF="true"/>
			<WP enName="Solar"			ONOFF="true"/>
			<WP enName="Fish"			ONOFF="true"/>
			<WP enName="Transformer"	ONOFF="true"/>
		</SETTING>
		<SETTING>
			<PATH ORI_PATH="fscommand\WP\ORI\"
				  CUSTOM_PATH="fscommand\CUSTOM\"
				  SRC_PATH="fscommand\WP\SRC\"
				  Note="This is setup for system."/>
			<PATH ORI_PATH="WP\ORI\"
				  CUSTOM_PATH="CUSTOM\"
				  SRC_PATH="..\..\"
				  Note="This line is setup for output function."/>
		</SETTING>
	</SYSTEM>
	<LANGUAGE DefaultIndex="0"
			  iTotalLanguage="3">
		<MENU Language="繁體中文"
			  LanguageIndex="0">
			<TITLE Name="檔案">
				<SUB Name="讀取圖片"
					 Cmd="LoadPng"/>
				<SUB Name="輸出"
					 Cmd="Output"/>
				<SUB type="separator"/>
				<SUB Name="儲存編輯結果"
					 Cmd="Save"/>
				<SUB type="separator"/>
				<SUB Name="離開"
					 Cmd="QUIT"/>
			</TITLE>
			<TITLE Name="檢視">
				<SUB Name="原廠貼圖"
					 Cmd="ViewPIC"
					 type="check"
					 toggled="true"/>
				<SUB Name="當前貼圖"
					 Cmd="ViewPIC"
					 type="check"
					 toggled="true"/>
				<SUB Name="已讀貼圖"
					 Cmd="ViewPIC"
					 type="check"
					 toggled="true"/>
				<SUB type="separator"/>
				<SUB Name="選擇目標桌布">
					<SUBSUB Name="含羞草"
							wpIndex="0"
							enName="Mimosa"
							Cmd="SwitchWP"
							type="radio"
							groupName="WP"
							toggled="false"/>
					<SUBSUB Name="搔女孩"
							wpIndex="1"
							enName="Girl"
							Cmd="SwitchWP"
							type="radio"
							groupName="WP"
							toggled="false"/>
					<SUBSUB Name="機械鐘"
							wpIndex="2"
							enName="Clock"
							Cmd="SwitchWP"
							type="radio"
							groupName="WP"
							toggled="false"/>
					<SUBSUB Name="百駿圖"
							wpIndex="3"
							enName="Horse"
							Cmd="SwitchWP"
							type="radio"
							groupName="WP"
							toggled="false"/>
					<SUBSUB Name="白日夢"
							wpIndex="4"
							enName="Daydream"
							Cmd="SwitchWP"
							type="radio"
							groupName="WP"
							toggled="false"/>
					<SUBSUB Name="燈塔"
							wpIndex="5"
							enName="Lighthouse"
							Cmd="SwitchWP"
							type="radio"
							groupName="WP"
							toggled="false"/>
					<SUBSUB Name="寵物狗"
							wpIndex="6"
							enName="Puppy"
							Cmd="SwitchWP"
							type="radio"
							groupName="WP"
							toggled="false"/>
					<SUBSUB Name="太陽系"
							wpIndex="7"
							enName="Solar"
							Cmd="SwitchWP"
							type="radio"
							groupName="WP"
							toggled="false"/>
					<SUBSUB Name="魚缸"
							wpIndex="8"
							enName="Fish"
							Cmd="SwitchWP"
							type="radio"
							groupName="WP"
							toggled="false"/>
					<SUBSUB Name="變形金剛"
							wpIndex="9"
							enName="Transformer"
							Cmd="SwitchWP"
							type="radio"
							groupName="WP"
							toggled="false"/>
				</SUB>
				<SUB type="separator"/>
				<SUB Name="預覽輸出"
					 Cmd="Preview"/>
				<SUB type="separator"/>
				<SUB Name="切換語系">
					<SUBSUB Name="繁體中文"
							Cmd="lan_tw"
							type="radio"
							groupName="LANGUAGE"
							toggled="true"/>
					<SUBSUB Name="简体中文"
							Cmd="lan_cn"
							type="radio"
							groupName="LANGUAGE"
							toggled="false"/>
					<SUBSUB Name="English"
							Cmd="lan_en"
							type="radio"
							groupName="LANGUAGE"
							toggled="false"/>
				</SUB>
			</TITLE>
			<TITLE Name="關於">
				<SUB Name="幫助"
					 Cmd="Help"/>
			</TITLE>
		</MENU>
		<OTHER Language="繁體中文"
			   LanguageIndex="0">
			<PB Title_ORI="原廠貼圖"
				Title_CUR="當前貼圖"
				Title_LOD="已讀貼圖"/>
			<CMD_BAR LB_PicTitle="目標貼圖:"
					 CB_PicWord="貼圖>>"
					 CB_PicBK="背景圖"
					 BT_Load="讀取貼圖"
					 LB_CurWP="當前桌布:"
					 BT_Use="套用已讀貼圖"
					 BT_Recover="還原為原廠貼圖"
					 BT_Preview="預覽輸出"
					 BT_Output="輸出"/>
			<PREVIEW Title="預覽輸出"/>
			<ALERT Title="系統訊息"
				   Message="請將CONFIG.xml檔, 存放於[fscommand]資料夾下"/>
			<ALERT Title="系統訊息"
				   Message="請將run.bat檔, 存放於[fscommand]資料夾下"/>
			<ALERT Title="系統訊息"
				   Message="轉換完畢, 檔案已覆蓋於原桌布資源檔上"/>
		</OTHER>

		<MENU Language="简体中文"
			  LanguageIndex="1">
			<TITLE Name="档案">
				<SUB Name="读取图片"
					 Cmd="LoadPng"/>
				<SUB Name="输出"
					 Cmd="Output"/>
				<SUB type="separator"/>
				<SUB Name="储存编辑结果"
					 Cmd="Save"/>
				<SUB type="separator"/>
				<SUB Name="离开"
					 Cmd="QUIT"/>
			</TITLE>
			<TITLE Name="检视">
				<SUB Name="原厂贴图"
					 Cmd="ViewPIC"
					 type="check"
					 toggled="true"/>
				<SUB Name="当前贴图"
					 Cmd="ViewPIC"
					 type="check"
					 toggled="true"/>
				<SUB Name="已读贴图"
					 Cmd="ViewPIC"
					 type="check"
					 toggled="true"/>
				<SUB type="separator"/>
				<SUB Name="选择目标桌布">
					<SUBSUB Name="含羞草"
							wpIndex="0"
							enName="Mimosa"
							Cmd="SwitchWP"
							type="radio"
							groupName="WP"
							toggled="false"/>
					<SUBSUB Name="搔女孩"
							wpIndex="1"
							enName="Girl"
							Cmd="SwitchWP"
							type="radio"
							groupName="WP"
							toggled="false"/>
					<SUBSUB Name="机械钟"
							wpIndex="2"
							enName="Clock"
							Cmd="SwitchWP"
							type="radio"
							groupName="WP"
							toggled="false"/>
					<SUBSUB Name="百骏图"
							wpIndex="3"
							enName="Horse"
							Cmd="SwitchWP"
							type="radio"
							groupName="WP"
							toggled="false"/>
					<SUBSUB Name="白日梦"
							wpIndex="4"
							enName="Daydream"
							Cmd="SwitchWP"
							type="radio"
							groupName="WP"
							toggled="false"/>
					<SUBSUB Name="灯塔"
							wpIndex="5"
							enName="Lighthouse"
							Cmd="SwitchWP"
							type="radio"
							groupName="WP"
							toggled="false"/>
					<SUBSUB Name="宠物狗"
							wpIndex="6"
							enName="Puppy"
							Cmd="SwitchWP"
							type="radio"
							groupName="WP"
							toggled="false"/>
					<SUBSUB Name="太阳系"
							wpIndex="7"
							enName="Solar"
							Cmd="SwitchWP"
							type="radio"
							groupName="WP"
							toggled="false"/>
					<SUBSUB Name="鱼缸"
							wpIndex="8"
							enName="Fish"
							Cmd="SwitchWP"
							type="radio"
							groupName="WP"
							toggled="false"/>
					<SUBSUB Name="变形金刚"
							wpIndex="9"
							enName="Transformer"
							Cmd="SwitchWP"
							type="radio"
							groupName="WP"
							toggled="false"/>
				</SUB>
				<SUB type="separator"/>
				<SUB Name="预览输出"
					 Cmd="Preview"/>
				<SUB type="separator"/>
				<SUB Name="切换语系">
					<SUBSUB Name="繁體中文"
							Cmd="lan_tw"
							type="radio"
							groupName="LANGUAGE"
							toggled="false"/>
					<SUBSUB Name="简体中文"
							Cmd="lan_cn"
							type="radio"
							groupName="LANGUAGE"
							toggled="true"/>
					<SUBSUB Name="English"
							Cmd="lan_en"
							type="radio"
							groupName="LANGUAGE"
							toggled="false"/>
				</SUB>
			</TITLE>
			<TITLE Name="关于​">
				<SUB Name="​帮助"
					 Cmd="Help"/>
			</TITLE>
		</MENU>
		<OTHER Language="简体中文"
			   LanguageIndex="1">
			<PB Title_ORI="原厂贴图"
				Title_CUR="当前贴图"
				Title_LOD="已读贴图"/>
			<CMD_BAR LB_PicTitle="目标贴图:"
					 CB_PicWord="贴图>>"
					 CB_PicBK="背景图"
					 BT_Load="读取贴图"
					 LB_CurWP="当前桌布:"
					 BT_Use="套用已读贴图"
					 BT_Recover="还原为原厂贴图"
					 BT_Preview="预览输出"
					 BT_Output="输出"/>
			<PREVIEW Title="预览输出"/>
			<ALERT Title="系统讯息"
				   Message="请将CONFIG.xml档, 存放于[fscommand]资料夹下"/>
			<ALERT Title="系统讯息"
				   Message="请将run.bat档, 存放于[fscommand]资料夹下"/>
			<ALERT Title="系统讯息"
				   Message="转换完毕, 档案已覆盖于原桌布资源档上"/>
		</OTHER>

		<MENU Language="English"
			  LanguageIndex="2">
			<TITLE Name="File">
				<SUB Name="Load"
					 Cmd="LoadPng"/>
				<SUB Name="Output"
					 Cmd="Output"/>
				<SUB type="separator"/>
				<SUB Name="Save edit"
					 Cmd="Save"/>
				<SUB type="separator"/>
				<SUB Name="QUIT"
					 Cmd="QUIT"/>
			</TITLE>
			<TITLE Name="View">
				<SUB Name="Original texture"
					 Cmd="ViewPIC"
					 type="check"
					 toggled="true"/>
				<SUB Name="Current texture"
					 Cmd="ViewPIC"
					 type="check"
					 toggled="true"/>
				<SUB Name="Loaded texture"
					 Cmd="ViewPIC"
					 type="check"
					 toggled="true"/>
				<SUB type="separator"/>
				<SUB Name="Choose Target Wallpaper">
					<SUBSUB Name="Mimosa"
							wpIndex="0"
							enName="Mimosa"
							Cmd="SwitchWP"
							type="radio"
							groupName="WP"
							toggled="false"/>
					<SUBSUB Name="Girl"
							wpIndex="1"
							enName="Girl"
							Cmd="SwitchWP"
							type="radio"
							groupName="WP"
							toggled="false"/>
					<SUBSUB Name="Clock"
							wpIndex="2"
							enName="Clock"
							Cmd="SwitchWP"
							type="radio"
							groupName="WP"
							toggled="false"/>
					<SUBSUB Name="Horse"
							wpIndex="3"
							enName="Horse"
							Cmd="SwitchWP"
							type="radio"
							groupName="WP"
							toggled="false"/>
					<SUBSUB Name="Daydream"
							wpIndex="4"
							enName="Daydream"
							Cmd="SwitchWP"
							type="radio"
							groupName="WP"
							toggled="false"/>
					<SUBSUB Name="Lighthouse"
							wpIndex="5"
							enName="Lighthouse"
							Cmd="SwitchWP"
							type="radio"
							groupName="WP"
							toggled="false"/>
					<SUBSUB Name="Puppy"
							wpIndex="6"
							enName="Puppy"
							Cmd="SwitchWP"
							type="radio"
							groupName="WP"
							toggled="false"/>
					<SUBSUB Name="Solar"
							wpIndex="7"
							enName="Solar"
							Cmd="SwitchWP"
							type="radio"
							groupName="WP"
							toggled="false"/>
					<SUBSUB Name="Fish"
							wpIndex="8"
							enName="Fish"
							Cmd="SwitchWP"
							type="radio"
							groupName="WP"
							toggled="false"/>
					<SUBSUB Name="Transformer"
							wpIndex="9"
							enName="Transformer"
							Cmd="SwitchWP"
							type="radio"
							groupName="WP"
							toggled="false"/>
				</SUB>
				<SUB type="separator"/>
				<SUB Name="Preview"
					 Cmd="Preview"/>
				<SUB type="separator"/>
				<SUB Name="Language">
					<SUBSUB Name="繁體中文"
							Cmd="lan_tw"
							type="radio"
							groupName="LANGUAGE"
							toggled="false"/>
					<SUBSUB Name="简体中文"
							Cmd="lan_cn"
							type="radio"
							groupName="LANGUAGE"
							toggled="false"/>
					<SUBSUB Name="English"
							Cmd="lan_en"
							type="radio"
							groupName="LANGUAGE"
							toggled="true"/>
				</SUB>
			</TITLE>
			<TITLE Name="About">
				<SUB Name="Help"
					 Cmd="Help"/>
			</TITLE>
		</MENU>
		<OTHER Language="English"
			   LanguageIndex="2">
			<PB Title_ORI="Original texture"
				Title_CUR="Current texture"
				Title_LOD="Loaded texture"/>
			<CMD_BAR LB_PicTitle="Target texture:"
					 CB_PicWord="texture>>"
					 CB_PicBK="background"
					 BT_Load="Load"
					 LB_CurWP="Current Wallpaper:"
					 BT_Use="Use Loaded texture"
					 BT_Recover="Recover"
					 BT_Preview="Preview"
					 BT_Output="Output"/>
			<PREVIEW Title="Preview"/>
			<ALERT Title="System"
				   Message="Please save the file of CONFIG.xml, into the folder [fscommand]"/>
			<ALERT Title="System"
				   Message="Please save the file of run.bat, into the folder [fscommand]"/>
			<ALERT Title="System"
				   Message="Convert done, resource file overwrited."/>
		</OTHER>
	</LANGUAGE>
